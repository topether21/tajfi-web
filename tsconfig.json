{
	"extends": "expo/tsconfig.base",
	"compilerOptions": {
		"module": "esnext",
		"strict": true,
		"paths": {
			"@/*": ["./*"],
			"tailwind.config": ["./tailwind.config.js"]
		},
		"skipLibCheck": true
	},
	"include": [
		"**/*.ts",
		"**/*.tsx",
		".expo/types/**/*.ts",
		"expo-env.d.ts",
		"nativewind-env.d.ts"
	],
	"exclude": [
		"components/ui/**/*",
		"node_modules",
		"dist",
		"test",
		"labs",
		"_legacy",
		"_app_onboard/**/*.{ts,tsx}"
	]
}
