{
	"extends": "expo/tsconfig.base",
	"compilerOptions": {
		"module": "esnext",
		"strict": true,
		"paths": {
			"@/*": ["./*"],
			"tailwind.config": ["./tailwind.config.js"]
		}
	},
	"exclude": [
		"node_modules",
		"dist",
		"test",
		"labs",
		"_legacy",
		"components/ui/**/*.ts",
		"components/ui/**/*.tsx",
		"_app_onboard/**/*.ts",
		"_app_onboard/**/*.tsx"
	],
	"include": [
		"**/*.ts",
		"**/*.tsx",
		".expo/types/**/*.ts",
		"expo-env.d.ts",
		"nativewind-env.d.ts"
	]
}
